
class Destination {
  String name;
  String code;
  float avgFlightsPerDay;
  int durationMin;
  int durationMax;

  Destination(String name, String code, float avgFlightsPerDay, int durationMin, int durationMax) {
    this.name = name;
    this.code = code;
    this.avgFlightsPerDay = avgFlightsPerDay;
    this.durationMin = durationMin;
    this.durationMax = durationMax;
  }
  
  
  
}